pipeline {
  agent any
  stages {
    stage('StageOne') {
      steps {
        sh 'echo "This the Stage One"'
      }
    }

    stage('StageTwo') {
      steps {
        sh 'echo "This is Stage Two"'
      }
    }

    stage('') {
      steps {
        echo 'Done with Pipeline'
      }
    }

  }
}